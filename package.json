    "bugs": {
        "url": "https://github.com/fnick851/tabluea_WDC_prototype/issues"
    },
    "homepage": "https://github.com/fnick851/tabluea_WDC_prototype#readme",
    "dependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/polyfill": "^7.4.4",
        "@babel/preset-env": "^7.5.5",
        "axios": "^0.19.0",
        "flat": "^4.1.0",
        "npm-watch": "^0.6.0"
    }
}
