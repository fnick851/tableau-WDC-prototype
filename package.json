{
    "name": "wdcs",
    "version": "1.0.0",
    "description": "To develop locally, you need to download the WDC simulator.",
    "main": "index.js",
    "watch": {
        "build": "js/src.js"
    },
    "scripts": {
        "dev": "npm-watch build",
        "browserify": "browserify js/src.js -o js/browserified.js",
        "babelify": "npx babel js/browserified.js -o js/bundle.js",
        "build": "npm run browserify && npm run babelify && rm js/browserified.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fnick851/tabluea_WDC_prototype.git"
    },
